<config>
  <modules>
    <Tcs_Codshippingbooking>
      <version>0.0.1</version>
    </Tcs_Codshippingbooking>
  </modules>
  <global>
    <helpers>
      <codshippingbooking>
        <class>Tcs_Codshippingbooking_Helper</class>
      </codshippingbooking>
    </helpers>
		<models>
			<codshippingbooking>
					<class>Tcs_Codshippingbooking_Model</class>
			</codshippingbooking>
		</models>
		<blocks>
			<codshippingbooking>
				<class>Tcs_Codshippingbooking_Block</class>
			</codshippingbooking>
			<!--sales>
				<rewrite>
					<order_view>Ubercx_Shippingtracking_Block_Order_View</order_view>
				  <order_shipment_items>Ubercx_Shippingtracking_Block_Order_Shipment_Items</order_shipment_items>
				</rewrite>
			</sales>
			<adminhtml>
				<rewrite>
					<sales_order_shipment_view_tracking>Ubercx_Shippingtracking_Block_Adminhtml_Sales_Order_Shipment_View_Tracking</sales_order_shipment_view_tracking>
				</rewrite>
			</adminhtml-->
		</blocks>
  </global>
	<frontend>
		<routers>
			<codshippingbooking>
				<use>standard</use>
				<args>
					<module>Tcs_Codshippingbooking</module>
					<frontName>codshippingbooking</frontName>
				</args>
			</codshippingbooking>
		</routers>
		<layout>
			<updates>
				<codshippingbooking>
					<file>codshippingbooking.xml</file>
				</codshippingbooking>
			</updates>
		</layout>
	</frontend>
	<admin>
		<routers>
			<tcs_codshippingbooking>
				<use>admin</use>
				<args>
					<module>Tcs_Codshippingbooking</module>
					<frontName>admin_codshippingbooking</frontName>
				</args>
			</tcs_codshippingbooking>
		</routers>
	</admin>
	<adminhtml>
		<layout>
			<updates>
				<codshippingbooking>
					<file>codshippingbooking.xml</file>
				</codshippingbooking>
			</updates>
		</layout>
	</adminhtml>
</config> 
